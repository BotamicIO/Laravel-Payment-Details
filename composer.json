{
    "name": "artisanry/payment-details",
    "description": "Easy way of storing payment details for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-payment-details",
        "Laravel Messageable"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "artisanry/eloquent-models": "^2.0.0",
        "illuminate/database": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\PaymentDetails\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\PaymentDetails\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\PaymentDetails\\PaymentDetailsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
